#include<stdio.h>

int main(){
	int i,j,n,array[10],deleted;
	
	printf("Enter the no of element:");
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
		printf("array[%d]=",i);
		scanf("%d",&array[i]);
	}
	printf("Enter the number to be deleted:");
	scanf("%d",&deleted);
	
	for(i=0;i<n;i++){
		if(array[i]== deleted){
			for(j=i;j<n;j++){
				array[j]=array[j+1];
			}
		}
		
	
	}
		printf("\nThe array after delection is ");
	for(j=0;j<n-1;j++){
		printf("\narray[%d]=%d",j,array[j]);	
	
	
	
	}
	return 0;
	
	
}
