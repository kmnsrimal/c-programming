#include<stdio.h>
 int fibo(int n);
 
 main(){
 
     int a;
     printf("Enter a Value: ");
     scanf("%d",&a);
     int x;
    for (x=0 ; x<a ; x++){
      printf("%d ", fibo(x));
	 }
 }
	 
 int fibo(int i){
 	
	if(i==0)
 	{
 		return 0;
	}
	
	if(i==1)
	{
		return 1;	
	}
	
	else 
	{
		return fibo(i-1) + fibo(i-2);
	}
 	
 }	 
	 
 
